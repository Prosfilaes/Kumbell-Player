project Kumbell_Kalah4 is

   for Languages use ("ada", "c");

   for Source_Dirs use ("src", "src/kalah", "src/kalah4", "src/mmap");
   for Main use ("build_from_start.adb");
   for Object_Dir use "obj";
   for Exec_Dir use ".";

   package Compiler is
           for Default_Switches ("Ada") use ("-O2", "-Wall", "-W", "-gnat2022", "-march=native", "-mtune=native", "-gnata");
   end Compiler;

end Kumbell_Kalah4;

